<?xml version="1.0" encoding="UTF-8"?>
<gmfgraph:Canvas xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfgraph="http://www.eclipse.org/gmf/2005/GraphicalDefinition" name="simplenet">
  <figures
      name="Default">
    <figures
        xsi:type="gmfgraph:Rectangle"
        referencingElements="Device"
        name="DeviceFigure">
      <children
          xsi:type="gmfgraph:Label"
          referencingElements="DeviceName"
          name="DeviceNameFigure"
          text="&lt;...>"/>
      <children
          xsi:type="gmfgraph:Label"
          referencingElements="DeviceModel"
          name="DeviceModelFigure"
          text="&lt;...>"/>
    </figures>
    <figures
        xsi:type="gmfgraph:Rectangle"
        referencingElements="Module"
        name="ModuleFigure">
      <children
          xsi:type="gmfgraph:Label"
          referencingElements="ModuleName"
          name="ModuleNameFigure"
          text="&lt;...>"/>
    </figures>
    <figures
        xsi:type="gmfgraph:Rectangle"
        referencingElements="Port"
        name="PortFigure">
      <children
          xsi:type="gmfgraph:Label"
          referencingElements="PortName"
          name="PortNameFigure"
          text="&lt;...>"/>
    </figures>
    <figures
        xsi:type="gmfgraph:Rectangle"
        referencingElements="ModulesCompartment PortsCompartment"
        name="SimpleRectangle"/>
  </figures>
  <nodes
      name="Device"
      figure="DeviceFigure"/>
  <nodes
      name="Module"
      figure="ModuleFigure"/>
  <nodes
      name="Port"
      figure="PortFigure"/>
  <compartments
      name="ModulesCompartment"
      figure="SimpleRectangle"/>
  <compartments
      name="PortsCompartment"
      figure="SimpleRectangle"/>
  <labels
      name="DeviceName"
      figure="DeviceNameFigure"/>
  <labels
      name="DeviceModel"
      figure="DeviceModelFigure"/>
  <labels
      name="ModuleName"
      figure="ModuleNameFigure"/>
  <labels
      name="PortName"
      figure="PortNameFigure"/>
</gmfgraph:Canvas>
