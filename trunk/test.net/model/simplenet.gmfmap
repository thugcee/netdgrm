<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2005/mappings/2.0"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="simplenet.ecore#//Net/devices"/>
    <ownedChild>
      <domainMetaElement
          href="simplenet.ecore#//Device"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="simplenet.gmfgraph#DeviceName"/>
        <features
            href="simplenet.ecore#//Device/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="simplenet.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="simplenet.gmfgraph#Device"/>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="simplenet.ecore#//Device/modules"/>
        <ownedChild>
          <domainMetaElement
              href="simplenet.ecore#//Module"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping">
            <diagramLabel
                href="simplenet.gmfgraph#ModuleName"/>
            <features
                href="simplenet.ecore#//Module/name"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="simplenet.gmftool#//@palette/@tools.0/@tools.1"/>
          <diagramNode
              href="simplenet.gmfgraph#Module"/>
          <children
              compartment="//@nodes.0/@ownedChild/@children.0/@ownedChild/@compartments.0">
            <containmentFeature
                href="simplenet.ecore#//Module/ports"/>
            <ownedChild>
              <domainMetaElement
                  href="simplenet.ecore#//Port"/>
              <labelMappings
                  xsi:type="gmfmap:FeatureLabelMapping">
                <diagramLabel
                    href="simplenet.gmfgraph#PortName"/>
                <features
                    href="simplenet.ecore#//Port/name"/>
              </labelMappings>
              <tool
                  xsi:type="gmftool:CreationTool"
                  href="simplenet.gmftool#//@palette/@tools.0/@tools.2"/>
              <diagramNode
                  href="simplenet.gmfgraph#Port"/>
            </ownedChild>
          </children>
          <compartments
              children="//@nodes.0/@ownedChild/@children.0/@ownedChild/@children.0">
            <compartment
                href="simplenet.gmfgraph#PortsCompartment"/>
          </compartments>
        </ownedChild>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0">
        <compartment
            href="simplenet.gmfgraph#ModulesCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <diagram>
    <diagramCanvas
        href="simplenet.gmfgraph#simplenet"/>
    <domainModel
        href="simplenet.ecore#/"/>
    <domainMetaElement
        href="simplenet.ecore#//Net"/>
    <palette
        href="simplenet.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
